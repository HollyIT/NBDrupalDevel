                <attr name="displayName" stringvalue="drupal-devel-action"/>
            
                <attr name="instanceCreate" methodvalue="org.hollyit.drupaldevel.Util.alwaysEnabled"/>
                <attr name="delegate" newvalue="org.hollyit.drupaldevel.DrupalDevelTool$DrupalDevelAction"/>
