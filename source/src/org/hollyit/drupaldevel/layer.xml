    <folder name="Editors">
        <folder name="Toolbars">
            <folder name="Default">
            <!--
            <file name="HorizontalGlue.instance">
               <attr name="instanceCreate" methodvalue="javax.swing.Box.createHorizontalGlue" />
               <attr name="position" intvalue="100000"/>
            </file>
            -->
                <file name="separator-before-drupal-devel-action.instance">
                    <attr name="instanceCreate" newvalue="javax.swing.JSeparator"/>
